{
    "name": "app/symfony-ddd",
    "description": "WebMakers - PHP Symfony Developer Recruitment Task",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^8.2",
        "ext-pdo": "*",
        "doctrine/doctrine-bundle": "^2.16",
        "doctrine/doctrine-migrations-bundle": "^3.4",
        "doctrine/migrations": "^3.9",
        "doctrine/orm": "^2.20",
        "ramsey/uuid-doctrine": "^1.8",
        "symfony/framework-bundle": "^6.4",
        "symfony/uid": "^6.4",
        "symfony/yaml": "^6.4",
        "twig/twig": "^3.21"
    },
    "require-dev": {
        "symfony/debug-bundle": "^6.4",
        "symfony/maker-bundle": "^1.64",
        "symfony/profiler-pack": "^1.0",
        "phpunit/phpunit": "^10.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {},
        "post-install-cmd": [],
        "post-update-cmd": []
    },
    "config": {
        "platform": {
            "php": "8.2.29"
        },
        "preferred-install": "dist",
        "sort-packages": true
    }
}
